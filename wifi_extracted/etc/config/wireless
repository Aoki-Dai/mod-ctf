config wifi-device 'radio0'
    option type 'mac80211'
    option channel '11'
    option hwmode '11g'
    option path 'platform/ar933x_wmac'

config wifi-iface
    option device 'radio0'
    option network 'lan'
    option mode 'ap'
    option ssid 'MOD-Device-AP'
    option encryption 'psk2'
    option key 'dummy{n0t_th3_r34l_p4ssw0rd}'
